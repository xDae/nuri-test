{
  "version": "0.0.1",
  "private": true,
  "description": "nuri graphql server",
  "license": "Apache 2.0",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "dev": "yarn generate && NODE_ENV=development tsnd --respawn src/index.ts",
